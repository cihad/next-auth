{
  "$schema": "https://turborepo.com/microfrontends/schema.json",
  "applications": {
    "@fakestore/home": {
      "packageName": "@fakestore/home",
      "development": {
        "local": 3000,
        "fallback": "microfrontends-marketing.labs.vercel.dev"
      }
    },
    "@fakestore/cart": {
      "packageName": "@fakestore/cart",
      "development": {
        "local": 3001
      },
      "routing": [
        {
          "group": "cart",
          "paths": ["/cart", "/cart/:path*"]
        }
      ]
    }
  }
}
